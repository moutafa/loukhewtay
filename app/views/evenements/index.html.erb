<h1>Listing evenements</h1>

<table>
  <tr>
    <th>Nom</th>
    <th>Description</th>
    <th>Date</th>
    <th>Frequence</th>
    <th>Confidentialite</th>
    <th>Endroit</th>
    <th>Categorieevent</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @evenements.each do |evenement| %>
  <tr>
    <td><%= evenement.nom %></td>
    <td><%= evenement.description %></td>
    <td><%= evenement.date %></td>
    <td><%= evenement.frequence %></td>
    <td><%= evenement.confidentialite %></td>
    <td><%= evenement.endroit_id %></td>
    <td><%= evenement.categorieEvent_id %></td>
    <td><%= evenement.user_id %></td>
    <td><%= link_to 'Show', evenement %></td>
    <td><%= link_to 'Edit', edit_evenement_path(evenement) %></td>
    <td><%= link_to 'Destroy', evenement, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Evenement', new_evenement_path %>
